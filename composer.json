{
    "name": "gustavosf/the-simple-php-framework",
    "description": "The Simple PHP Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Seganfredo",
            "email": "gustavosf@gmail.com"
        }
    ],
    "repositories": [{
        "type": "git",
        "name": "gustavosf/the-simple-php-framework-kernel",
        "url": "https://github.com/gustavosf/the-simple-php-framework-kernel"
    }],
    "require": {
        "php": ">=5.4.0",
        "gustavosf/the-simple-php-framework-kernel": "dev-master"
    },
    "require-dev": {
        "behat/behat": "~3.0.6",
        "behat/mink": "1.6.*",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "vendor-dir": "app/vendor"
    },
    "autoload": {
        "psr-4": {
            "Model\\": "app/models"
        }
    }
}
